{
  "name": "ShaqBlocker",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "declarativeNetRequest"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "action": {
    "default_popup": "ShaqBlocker.html",
    "default_icon": "icon.png"
    }
}
